{
  "routes": [
    {
      "route": "/index.html",
      "methods": [
        "get"
      ]
    },
    {
      "route": "/*",
      "redirect": "/redirect.js"
    },
    {
      "navigationFallback": {
        "rewrite": "/redirect.js"
      },
      "responseOverrides": {
        "404": {
          "rewrite": "/redirect.js"
        }
      }
    }
  ]
}