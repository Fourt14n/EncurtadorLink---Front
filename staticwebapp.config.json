{
  "routes": [
    {
      "route": "/index.html",
      "serve": "/index.html"
    },
    {
      "route": "/common/*",
      "serve": "/common/*"
    },
    {
      "route": "/*",
      "serve": "/common/redirect.html",
      "statusCode": 200
    }
  ],
  "responseOverrides": {
    "404": {
      "serve": "/common/redirect.html",
      "statusCode": 200
    }
  }
}
